#include<bits/stdc++.h>
using namespace std

int dp[1000][1000];                                       

int solve(int a[],int m,int n)
{
    if(m < 0 && n > 0)
        return 0;
    if(n == 0)
        return 1;
    if(n < 0)
        return 0;
    if(dp[m][n] != -1)
        return dp[m][n];

return dp[m][n] = solve(a,m,n-a[m]) + solve(a,m-1,n);
}

    int count_num(int a[], int t, int m)
    {
        for(int i=0;i<=t;i++)
        {
            for(int j=0;j<=m;j++)
            {
                dp[i][j] = -1;
            }
        }
        return solve(a,t-1,m);
    }

int main()
{
    int n,m,t;
    cin>>m>>n;
    int a[n];

    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }

    t = sizeof(a)/sizeof(a[0]);

    cout<<count_num(a,t,m);
}
